// from: https://en.wikipedia.org/w/index.php?title=YANG&oldid=792171830#Example
module example-sports <beginfold id='2'>{</beginfold id='2'>

  namespace "http://example.com/example-sports";
  prefix sports;

  import ietf-yang-types <beginfold id='2'>{</beginfold id='2'> prefix yang; <endfold id='2'>}</endfold id='2'>

  typedef season <beginfold id='2'>{</beginfold id='2'>
    type string;
    description
      "The name of a sports season, including the type and the year, e.g,
       'Champions League 2014/2015'.";
  <endfold id='2'>}</endfold id='2'>

  container sports <beginfold id='2'>{</beginfold id='2'>
    config true;

    list person <beginfold id='2'>{</beginfold id='2'>
      key name;
      leaf name <beginfold id='2'>{</beginfold id='2'> type string; <endfold id='2'>}</endfold id='2'>
      leaf birthday <beginfold id='2'>{</beginfold id='2'> type yang:date-and-time; mandatory true; <endfold id='2'>}</endfold id='2'>
    <endfold id='2'>}</endfold id='2'>

    list team <beginfold id='2'>{</beginfold id='2'>
      key name;
      leaf name <beginfold id='2'>{</beginfold id='2'> type string; <endfold id='2'>}</endfold id='2'>
      list player <beginfold id='2'>{</beginfold id='2'>
        key "name season";
        unique number;
        leaf name <beginfold id='2'>{</beginfold id='2'> type leafref <beginfold id='2'>{</beginfold id='2'> path "/sports/person/name"; <endfold id='2'>}</endfold id='2'>  <endfold id='2'>}</endfold id='2'>
        leaf season <beginfold id='2'>{</beginfold id='2'> type season; <endfold id='2'>}</endfold id='2'>
        leaf number <beginfold id='2'>{</beginfold id='2'> type uint16; mandatory true; <endfold id='2'>}</endfold id='2'>
        leaf scores <beginfold id='2'>{</beginfold id='2'> type uint16; default 0; <endfold id='2'>}</endfold id='2'>
      <endfold id='2'>}</endfold id='2'>
    <endfold id='2'>}</endfold id='2'>
  <endfold id='2'>}</endfold id='2'>
<endfold id='2'>}</endfold id='2'>
